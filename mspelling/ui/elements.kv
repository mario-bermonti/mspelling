<ThemedLabel@MDLabel>:
  font_size: "50sp"
  halign: "center"
  valign: "middle"
  text_size: (root.width - 25, None)

<ThemedButton@MDRaisedButton>:
  font_size: "50sp"

<ThemedTextInput@MDTextFieldRect>:
  font_size: "50sp"
  multiline: False

<ThemedLayout@MDBoxLayout>:
  size_hint: (0.6, 0.7)
  pos_hint: {"center_x": 0.5, "center_y":0.5}
  spacing: dp(10)